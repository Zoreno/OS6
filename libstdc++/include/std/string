#ifndef __STRING_
#define __STRING_

#pragma GCC system_header

#include <bits/c++config.hpp>
#include <bits/allocator.hpp>
#include <bits/char_traits.hpp>
#include <bits/cpp_type_traits.hpp>
#include <bits/stl_iterator_base_types.hpp>
#include <bits/stl_iterator_base_funcs.hpp>
#include <bits/stl_iterator.hpp>
#include <bits/stl_function.hpp>
#include <bits/numeric_traits.hpp>
#include <bits/stl_algobase.hpp>
#include <bits/basic_string.hpp>

OS6STD_BEGIN_SYSTEM_HEADER

namespace OS6STD
{
template <typename _T>
using t_string = basic_string<_T, char_traits<_T>, std::allocator<_T>>;

using string = t_string<char>;
}  // namespace OS6STD

OS6STD_END_SYSTEM_HEADER
#endif

